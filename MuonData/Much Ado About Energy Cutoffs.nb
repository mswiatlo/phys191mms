Cell["Bin the data->histogram with bin sizes of \"binwidth\"", "Text",
 CellChangeTimes->{{3.444410354941321*^9, 3.444410372590847*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"{", "num", "}"}], "=", 
   RowBox[{"Dimensions", "[", "rawdata", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"binwidth", "=", ".01"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"k", "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"x", "=", "1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"bins", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", 
       RowBox[{"2", "/", "binwidth"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Do", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"rawdata", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "\[GreaterEqual]", 
           RowBox[{"k", "*", "binwidth"}]}], "&&", 
          RowBox[{
           RowBox[{"rawdata", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "<", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"k", "+", "1"}], ")"}], "*", "binwidth"}]}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"x", "++"}], ";"}], "\[IndentingNewLine]", "}"}]}], 
        "\[IndentingNewLine]", "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", "num"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"bins", "[", 
       RowBox[{"[", 
        RowBox[{"k", "+", "1"}], "]"}], "]"}], "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         FractionBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "k"}], "+", "1"}], ")"}], "2"], "*", "binwidth"}], 
        ",", "x"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"x", "=", "0"}], ";"}], "\[IndentingNewLine]", "}"}], 
   "\[IndentingNewLine]", ",", 
   RowBox[{"{", 
    RowBox[{"k", ",", "0", ",", 
     RowBox[{
      RowBox[{"2", "/", "binwidth"}], "-", "1"}]}], "}"}]}], 
  "\[IndentingNewLine]", "]"}]}], "Input",
